{
  "name": "bonde-styleguide",
  "version": "0.16.5",
  "main": "dist/index.js",
  "private": false,
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/runtime": "^7.8.7",
    "babel-plugin-styled-components": "^1.10.6",
    "prop-types": "^15.7.2",
    "recharts": "^1.6.2",
    "styled-components": "^4.3.2",
    "webfontloader": "^1.6.28",
    "snyk": "^1.316.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir dist --copy-files",
    "prepare": "yarn run snyk-protect && yarn build",
    "dev": "yarn build --watch",
    "build:server": "styleguidist build",
    "test": "echo \"Error: no test specified\" && exit 0",
    "eject": "react-scripts eject",
    "start": "styleguidist server",
    "snyk-protect": "snyk protect"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "babel-plugin-styled-components",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-react-app-babel-7": "^4.0.2-0",
    "cross-env": "^5.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "react-styleguidist": "^9.1.13",
    "stylelint-config-styled-components": "^0.1.1",
    "webpack": "^4.38.0"
  },
  "snyk": true
}
