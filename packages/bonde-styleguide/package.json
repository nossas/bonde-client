{
  "name": "bonde-styleguide",
  "version": "0.14.7-alpha.0",
  "license": "AGPL-3.0",
  "private": false,
  "dependencies": {
    "autoprefixer": "8.6.5",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-preset-react-app": "3.1.2",
    "babel-register": "^6.26.0",
    "babel-runtime": "6.26.0",
    "bonde-webpage": "0.14.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.0",
    "eslint-plugin-react": "7.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "2.29.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.6",
    "promise": "8.0.1",
    "prop-types": "15.6.2",
    "raf": "3.4.0",
    "react": "16.4.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "16.4.2",
    "react-styleguidist": "7.0.20",
    "recharts": "1.0.1",
    "style-loader": "0.21.0",
    "styled-components": "3.3.3",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.0.1",
    "webfontloader": "^1.6.28",
    "webpack": "3.8.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.4"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "cross-env NODE_ENV=production babel src --out-dir dist --copy-files",
    "watch": "yarn prepare --watch",
    "dev": "styleguidist server",
    "build": "styleguidist build",
    "test": "cross-env NODE_ENV=test ava"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "sources": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "compileEnhancements": false,
    "require": [
      "babel-register",
      "./config/setupBrowserEnv.js"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-a11y/href-no-hash": [
        0
      ]
    }
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jsdom": "11.11.0",
    "require-hacker": "^3.0.1"
  }
}
